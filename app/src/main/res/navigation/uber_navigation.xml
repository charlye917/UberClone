<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/uber_navigation"
    app:startDestination="@id/profilingFragment">

    <fragment
        android:id="@+id/profilingFragment"
        android:name="com.charlye934.uber.login.presentation.fragment.ProfilingFragment"
        android:label="fragment_profiling"
        tools:layout="@layout/fragment_profiling" >
        <action
            android:id="@+id/action_profilingFragment_to_selectOptionAuthFragment"
            app:destination="@id/selectOptionAuthFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectOptionAuthFragment"
        android:name="com.charlye934.uber.login.presentation.fragment.SelectOptionAuthFragment"
        android:label="fragment_select_option_auth"
        tools:layout="@layout/fragment_select_option_auth" >
        <action
            android:id="@+id/action_selectOptionAuthFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_selectOptionAuthFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.charlye934.uber.login.presentation.fragment.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.charlye934.uber.login.presentation.fragment.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
</navigation>